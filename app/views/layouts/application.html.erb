<!DOCTYPE html>
<html>
  <head>
    <title>RocketElevatorsInformationSystem</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-154786688-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-154786688-1');
    </script>

    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <!-- HEADER -->
    <%= render partial: 'common/header', locals: { navs: @navs}%>

    <% if notice %>
      <p class="alert alert-success"><%= notice %></p>
    <% end %>

    <%= yield %>

    <!-- FOOTER -->
    <%= render partial: 'common/footer' %>

  </body>



  <script>
    var plugin_path = 'assets/plugins/';
  </script>

</html>